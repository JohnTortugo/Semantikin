void soma() {
	int add = 3 - 2;
	int sub = 10 - 8;
	int mul = 5 * 6;
	int div = 9 / 3;
	int mod = 10 % 3;
	
	add += 1 * add;
	sub -= 1 - sub;
	mul *= 1 - mul;
	div /= 1 - div;
	mod %= 1 - mod;
	
	sub = 3 * 2;
	sub = 3;
	sub = add;
}

